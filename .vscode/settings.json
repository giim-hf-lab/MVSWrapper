    "${workspaceFolder}/externals/argparse/include",
    "${workspaceFolder}/externals/fmt/include",
    "${workspaceFolder}/externals/mio/include",
    "${workspaceFolder}/externals/onnxruntime/include",
    "${workspaceFolder}/externals/spdlog/include",
    "${workspaceFolder}/include"
  ]
}
