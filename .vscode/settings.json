  "cmake.buildDirectory": "${workspaceFolder}/build/project",
  "cmake.generator": "Ninja",
  "C_Cpp.autoAddFileAssociations": false,
  "C_Cpp.default.cStandard": "c17",
  "C_Cpp.default.cppStandard": "c++20",
  "C_Cpp.default.includePath": [
    "/usr/include/opencv4",
